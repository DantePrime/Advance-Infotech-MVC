@model IEnumerable<IP_App.Models.Plan_Details>

@{
    ViewBag.Title = "SearchPlanBySpeedResult";
    Layout = "~/Views/Shared/_LayoutPlan.cshtml";
}

<h2>Search Plan By Speed Result</h2>

<p>
    @Html.ActionLink("Create New", "AddPlan")    ||   @Html.ActionLink("Back to List", "ListAllPlans")
</p>

<table class="table">
    <tr>
     
        <th>
            @Html.DisplayNameFor(model => model.Plan_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Usage_Limit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Speed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Renewal_Duration)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subscriptions_Included)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Plan_Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Installation_Charges)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
     
        <td>
            @Html.DisplayFor(modelItem => item.Plan_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Usage_Limit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Speed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Renewal_Duration)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Subscriptions_Included)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Plan_Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Installation_Charges)
        </td>
     
    </tr>
}

</table>
